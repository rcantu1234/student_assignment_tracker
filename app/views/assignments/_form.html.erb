<%= simple_form_for(@assignment) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :student_id, collection: student_collection %>
    <%= f.input :title %>
    <%= f.input :grade %>
    <%= f.input :completed %>
    <%= f.input :late %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
